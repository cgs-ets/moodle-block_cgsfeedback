{{#courses}}
<div data-userid = "{{userid}}" class = "cgsfeedback-course-container">
    <header class="st_table_header">
        <h2>Course: {{coursename}}</h2>
        <span class ="cgsfeedback-expand-icon" data-courseid = "{{courseid}}"><i class="fa fa-caret-down" title = "Show" data-courseid = "{{courseid}}"></i></span>
        <span class ="cgsfeedback-collapse-icon" hidden  data-courseid = "{{courseid}}"><i class="fa fa-caret-up" title = "Hide" data-courseid = "{{courseid}}"></i></span>
    </header>
 <div class="st_wrap_table">
 <br>
 <div class = "table-responsive" hidden id = "cgsfeedback-activities-container-{{courseid}}">
  <table class="table">
  <thead>
    <tr>
      <th scope="col">Assessment</th>
      <th scope="col">Grade</th>
      <th scope="col">Feedback</th>
    </tr>
  </thead>
  <tbody>
  {{#modules}}
    <tr>
      <th scope="row"><a href = "{{{moduleurl}}}" target = "_blank" title = "Go to {{modulename}}"> {{modulename}}</a></th>
      <td>{{finalgrade}}</td>
      <td>{{{feedback}}}</td>
    </tr>
    {{/modules}}
  </tbody>
</table>
</div>
</div>
{{/courses}}